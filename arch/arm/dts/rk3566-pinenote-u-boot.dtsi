#include "rk356x-u-boot.dtsi"

/ {
	chosen {
		u-boot,spl-boot-order = "same-as-spl", &sdhci;
	};
};

&i2c0 {
	bootph-pre-ram;
};

&rk817 {
	bootph-pre-ram;

	regulators {
		bootph-pre-ram;
	};
};

&sdhci {
	cap-mmc-highspeed;
};
