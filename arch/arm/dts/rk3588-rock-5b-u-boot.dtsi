#include "rk3588-u-boot.dtsi"

/ {
	aliases {
		mmc0 = &sdmmc;
	};

	chosen {
		u-boot,spl-boot-order = "same-as-spl", &sdmmc;
	};
};

&sdmmc {
	bus-width = <4>;
	bootph-pre-ram;
	u-boot,spl-fifo-mode;
	status = "okay";
};
