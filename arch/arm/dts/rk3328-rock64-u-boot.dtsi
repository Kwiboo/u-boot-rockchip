#include "rk3328-u-boot.dtsi"
#include "rk3328-sdram-lpddr3-1600.dtsi"

/ {
	smbios {
		compatible = "u-boot,sysinfo-smbios";

		smbios {
			system {
				manufacturer = "pine64";
				product = "rock64_rk3328";
			};

			baseboard {
				manufacturer = "pine64";
				product = "rock64_rk3328";
			};

			chassis {
				manufacturer = "pine64";
				product = "rock64_rk3328";
			};
		};
	};
};

&spi0 {
	bootph-pre-ram;
	bootph-some-ram;

	flash@0 {
		bootph-pre-ram;
		bootph-some-ram;
	};
};

&vcc_sd {
	bootph-pre-ram;
};
